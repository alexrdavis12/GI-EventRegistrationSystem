%nav.navbar.navbar-vertical-left
    %ul.nav.navbar-nav{:role => "tablist"}
        %li
            %a{"data-toggle" => "tab", :href => "#profile", :role => "tab"}
                %i.fa.class.fa-user.fa-fw.fa-lg
                %span Profile
        %li{:class=> "active"}
            %a{"data-toggle" => "tab", :href => "#events", :role => "tab", :style => "background: #FFE796"}
                %i.fa.fa-list-alt.fa-fw.fa-lg
                %span Registered Events
        %li
            %a{"data-toggle" => "tab", :href => "#vehicles", :role => "tab", :style => "background: #FFE796"}
                %i.fa.fa-car.fa-fw.fa-lg
                %span Vehicles
        %li
            %a{"data-toggle" => "tab", :href => "#impressions", :role => "tab"}
                %i.fa.class.fa-flag.fa-fw.fa-lg
                %span Impressions
